{
  "name": "electricity-consumption-dashboard",
  "version": "1.0.0",
  "description": "Dashboard to visualize UK electricity consumption 2009-2024",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run start-all\" \"npm run client\"",
    "server": "nodemon server.js",
    "start-electricity": "nodemon services/electricity/electricityServer.js",
    "start-gas": "nodemon services/gas/gasServer.js",
    "start-all": "concurrently \"npm run server\" \"npm run start-electricity\" \"npm run start-gas\"",
    "client": "cd ../Graph-Frontend && npm run serve",
    "build": "cd ../Graph-Frontend && npm run build",
    "install-all": "npm install && cd ../Graph-Frontend && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "vue",
    "apexcharts",
    "electricity",
    "dashboard",
    "data-visualization"
  ],
  "author": "Your Name",
  "license": "MIT"
} 